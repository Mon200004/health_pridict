{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/predict",
      "dest": "api/index.py"
    },
    {
      "src": "/api/notify-critical-patients",
      "dest": "api/index.py"
    },
    {
      "src": "/",
      "dest": "api/index.py"
    }
  ]
}
